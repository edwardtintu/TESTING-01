public class UntitledTest {
  private WebDriver driver;
  private Map<String, Object> vars;
  JavascriptExecutor js;
  @Before
  public void setUp() {
    driver = new ChromeDriver();
    js = (JavascriptExecutor) driver;
    vars = new HashMap<String, Object>();
  }
  @After
  public void tearDown() {
    driver.quit();
  }
  public String waitForWindow(int timeout) {
    try {
      Thread.sleep(timeout);
    } catch (InterruptedException e) {
      e.printStackTrace();
    }
    Set<String> whNow = driver.getWindowHandles();
    Set<String> whThen = (Set<String>) vars.get("window_handles");
    if (whNow.size() > whThen.size()) {
      whNow.removeAll(whThen);
    }
    return whNow.iterator().next();
  }
  @Test
  public void untitled() {
    driver.get("https://www.cmch-vellore.edu/");
    driver.manage().window().setSize(new Dimension(974, 1032));
    vars.put("window_handles", driver.getWindowHandles());
    driver.findElement(By.cssSelector(".menu2")).click();
    vars.put("win1999", waitForWindow(2000));
    driver.switchTo().window(vars.get("win1999").toString());
    driver.findElement(By.id("txtHNum")).click();
    driver.findElement(By.id("txtHNum")).sendKeys("12345");
    driver.findElement(By.id("txtYrBirth")).click();
    driver.findElement(By.id("txtYrBirth")).sendKeys("12345");
    driver.findElement(By.cssSelector("li:nth-child(2) > a")).click();
    driver.findElement(By.id("txtwebtrans")).click();
    driver.findElement(By.id("txtwebtrans")).sendKeys("12345");
    driver.findElement(By.id("txtwebrandomno")).click();
    driver.findElement(By.id("txtwebrandomno")).sendKeys("12345");
    driver.findElement(By.id("btnLoginNew")).click();
  }
}
